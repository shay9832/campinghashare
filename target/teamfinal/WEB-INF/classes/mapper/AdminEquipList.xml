<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team.mvc.Interface.IAdminEquipListDAO">

    <select id="getList" resultType="com.team.mvc.DTO.AdminEquipListDTO">
            SELECT EQUIP_CODE,EQUIP_NAME,MAJOR_CATEGORY_NAME,MIDDLE_CATEGORY_NAME,ORIGINAL_PRICE,CREATED_DATE
            FROM VW_EQUIPMENT
    </select>

    <select id="getLists" resultType="com.team.mvc.DTO.AdminEquipListDTO">
            SELECT STOREN_ID, EQUIP_CODE, USER_CODE
                            , SIZE_ID, EQUIP_SIZE_NAME, REQUIRED_SPACE
                            , DAILY_STORAGE_FEE, STORE_MONTH
                            , STOREN_TITLE, STOREN_CONTENT
                            , DAILY_RENT_PRICE, CREATED_DATE
            FROM VW_STOREN
    </select>

</mapper>




