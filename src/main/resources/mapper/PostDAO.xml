<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team.mvc.Interface.IPostDAO">
    <resultMap id="postResultMap" type="com.team.mvc.DTO.PostDTO">
        <result column="게시물ID" property="postId"></result>

    </resultMap>




    <select id="listPostList" parameterType="com.team.mvc.DTO" resultType="com.team.mvc.DTO" resultMap="postResultMap">
        SELECT "게시물ID", "회원닉네임", "게시판카테고리", "말머리이름", "게시물제목", "게시물내용", "게시물생성일", "게시물조회수", "추천수", "댓글수"
        FROM VW_POST_MANAGEMENT
    </select>


</mapper>