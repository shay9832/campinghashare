<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team.mvc.AdminMainDAO">

    <resultMap id="adminMain" type="com.team.mvc.AdminMainDTO">
        <result column="신규회원수" property="newUserCount"/>
        <result column="스토렌결제수" property="storenPaymentCount"/>
        <result column="보관결제수" property="storagePaymentCount"/>
        <result column="렌탈결제수" property="rentalPaymentCount"/>
        <result column="총결제건수" property="totalPaymentCount"/>
        <result column="검수건수" property="inspectionCount"/>
        <result column="게시글신고수" property="postReportCount"/>
        <result column="댓글신고수" property="commentReportCount"/>
        <result column="총신고건수" property="totalReportCount"/>
        <result column="총회원수" property="totalUserCount"/>
    </resultMap>

    <select id="count" resultMap="adminMain">
        SELECT "신규회원수", "스토렌결제수", "보관결제수", "렌탈결제수",
               "총결제건수", "검수건수", "게시글신고수", "댓글신고수", "총신고건수", "총회원수"
        FROM V_DAILY_STATS
    </select>

</mapper>




